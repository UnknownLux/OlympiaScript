getgenv().infregen = true
getgenv().biceps = true
getgenv().musclepain = true 
getgenv().bench = true
getgenv().tricpes = true
getgenv().money = true
getgenv().abs = true
getgenv().pushups = true
getgenv().bench = false
getgenv().Legs = true
getgenv().backandleg = true

spawn(function()
    while infregen == true do 
        game:GetService("ReplicatedStorage").Remotes.EnergyFatigue.ServerRegen:FireServer()
        wait()
    end
end)

spawn(function()
    while biceps == true do 
        game:GetService("ReplicatedStorage").Remotes.Plus.Secondary.ServerPlusBic:FireServer()
        wait()
    end
end)

spawn(function()
    while musclepain == true do 
local args = {
    [1] = game:GetService("Players").LocalPlayer}

game:GetService("ReplicatedStorage").Pesos.AddPesos.RemoveAll:FireServer(unpack(args))

        wait()
    end
end)

spawn(function()
    while biceps == true do 
game:GetService("ReplicatedStorage").Remotes.Plus.Secondary.ServerPlusBic:FireServer()
game:GetService("ReplicatedStorage").Remotes.Plus.ServerPlusPeit:FireServer()
        wait()
    end
end)

spawn(function()
    while tricpes == true do 
            game:GetService("ReplicatedStorage").Remotes.Plus.ServerPlusTri:FireServer()
        wait()
    end
end)

spawn(function()
    while money == true do 
            game:GetService("ReplicatedStorage").Remotes.Money:FireServer()
        wait()
    end
end)

spawn(function()
    while abs == true do 
            game:GetService("ReplicatedStorage").Remotes.Plus.ServerPlusAb:FireServer()
            game:GetService("ReplicatedStorage").Remotes.Plus.ServerPlusAb:FireServer()
        wait()
    end
end)

spawn(function()
    while pushups == true do 
            game:GetService("ReplicatedStorage").Remotes.Plus.ServerPlusPeit:FireServer()
            game:GetService("ReplicatedStorage").Remotes.Plus.Secondary.ServerPlusBic:FireServer()
        wait()
    end
end)


spawn(function()
    while bench == true do 
            -- Script generated by SimpleSpy - credits to exx#9394
game:GetService("ReplicatedStorage").Remotes.Plus.Secondary.ServerPlusPeit:FireServer()

game:GetService("ReplicatedStorage").Remotes.Plus.ServerPlusTri:FireServer()

        wait()
    end
end)

spawn(function()
    while legs == true do 
            game:GetService("ReplicatedStorage").Remotes.Plus.ServerPlusLeg:FireServer()
        wait()
    end
end)

spawn(function()
    while backandleg == true do 
                game:GetService("ReplicatedStorage").Remotes.Plus.Secondary.ServerPlusBack:FireServer()
                game:GetService("ReplicatedStorage").Remotes.Plus.ServerPlusLeg:FireServer()
        wait()
    end
end)
